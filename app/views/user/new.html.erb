<div>
  <h1>新規ユーザ登録</h1>
  <% @user.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
  <%= form_tag("/signup") do %>
    <p>ユーザ名</p>
    <input name="name" >
    <p>パスワード</p>
    <input type="password" name="password" >
    <input type="submit" value="新規登録">
  <% end %>
  <%= link_to("戻る", "/login") %>
</div>
